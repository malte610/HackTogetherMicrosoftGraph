<AuthorizeView>
    <Authorized>
        <MudText>Hello, @context.User.Identity?.Name!</MudText>
        <MudIconButton Href="MicrosoftIdentity/Account/SignOut" Icon="@Icons.Material.Filled.Logout"></MudIconButton>
    </Authorized>
    <NotAuthorized>
        <MudIconButton Href="MicrosoftIdentity/Account/SignIn" Icon="@Icons.Material.Filled.Login"></MudIconButton>
    </NotAuthorized>
</AuthorizeView>
